{
    "linterOptions": {
        "format": "codeFrame"
    },
    "rules": {
        "max-line-length": [true, 320],
        "no-consecutive-blank-lines": true,
        "quotemark": [true, "single"],
        "semicolon": [true, "always"]
    },
    "jsRules": {
        "semicolon": [true, "always"]
    }
}